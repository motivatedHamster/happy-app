<h1>Quotes Index Page</h1>
<%= link_to "Add a Quote", new_quote_path %>
<% @quotes.each do |quote| %>
  <table>
    <tr>
      <td><%= quote.body %></td>
      <td><%= link_to "Edit", edit_quote_path(quote) %></td>
      <td><%= link_to "Delete", quote, method: :delete %></td>
    </tr>
  </table>
<% end %>

<%= render "home/nav" %>